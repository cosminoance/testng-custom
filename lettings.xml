<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="lettingSuite">
	<!-- Since we are declaring SuiteSetup as a class with @BeforeSuite it doesn't 
		matter which order we use -->
	<test thread-count="1" name="SuiteSetup">
		<classes>
			<class name="rightmove.letting.SuiteSetup" />
		</classes>
	</test>
	<test thread-count="1" name="LettingsE2E">
		<classes>
			<class name="rightmove.letting.LettingE2E" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
